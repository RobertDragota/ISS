<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #404040;" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="org.iss2024.bug_detection.Controllers.ValidatorTerminalController">
   <children>
      <TextArea fx:id="BugDescriptionField" layoutX="300.0" layoutY="59.0" prefHeight="93.0" prefWidth="262.0" />
      <TextField fx:id="BugNameField" layoutX="300.0" layoutY="187.0" promptText="bug_name" styleClass="rounded-textfield" stylesheets="@Validator_Terminal.css" />
      <Label layoutX="300.0" layoutY="26.0" text="Description" textFill="#81dd5f">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Button fx:id="UploadBugButton" layoutX="300.0" layoutY="252.0" mnemonicParsing="false" onAction="#HandleUploadBug" stylesheets="@Validator_Terminal.css" text="Upload_bug" />
      <TableView layoutX="-1.0" layoutY="-1.0" prefHeight="406.0" prefWidth="275.0" stylesheets="@Validator_Terminal.css">
        <columns>
          <TableColumn prefWidth="132.0" text="C1" />
          <TableColumn prefWidth="142.0" text="C2" />
        </columns>
      </TableView>
      <Button fx:id="LogoutButton" layoutX="476.0" layoutY="348.0" mnemonicParsing="false" onAction="#HandleLogout" prefHeight="46.0" prefWidth="110.0" stylesheets="@Validator_Terminal.css" text="Logout" />
   </children>
</AnchorPane>
